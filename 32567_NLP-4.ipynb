    "def preprocess(text):\n",
    "    stopwords_set = set(stopwords.words('english'))\n",
    "    return ''.join(\n",
    "    token.lower() for token in word_tokenize(text)\n",
    "    if token.isalpha() and token not in stopwords_set)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "d448959e",
   "metadata": {},
   "outputs": [],
   "source": [
    "vectorizer = TfidfVectorizer(ngram_range=(1,2), max_features=500)\n",
    "X = vectorizer.fit_transform(train_df['comment_text'].apply(preprocess))"
   ]
